    "my_tuple = (1, 2, 3, 4, 5, 6, 7, 8, 9) \n",
    "even = 0\n",
    "odd = 0\n",
    "for i in my_tuple:\n",
    "    if i % 2 == 0:\n",
    "        even+=1\n",
    "    else:\n",
    "        odd+=1\n",
    "print(\"total number of even numbers in my_tuple: \",even)\n",
    "print(\"total number of odd numbers in my_tuple:\",odd)"
   ]
